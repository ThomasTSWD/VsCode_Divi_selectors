{
  "name": "divi-suggest",
  "displayName": "Divi Suggest",
  "description": "Divi theme suggestions",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:css",
    "onLanguage:javascript"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "css",
        "extensions": [
          ".css"
        ],
        "aliases": [
          "CSS",
          "css"
        ]
      },
      {
        "id": "javascript",
        "extensions": [
          ".js"
        ],
        "aliases": [
          "JavaScript",
          "javascript"
        ]
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  },
  "publisher": "ThomasTSWD",
  "repository": {
    "type": "git",
    "url": "https://github.com/ThomasTSWD/VsCode_Divi_selectors.git"
  },
  "icon": "icon.png"
}